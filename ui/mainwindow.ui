    color: palette(highlighted-text);
}

QCheckBox {
	spacing: 8px;
	font-size: 14px;
}

QRadioButton {
	spacing: 8px;
	font-size: 14px;
}

QLineEdit {
	border: none;
	background-color: transparent;
	color: palette(text);
	font-size: 14px;
}

QScrollBar:vertical {
    background: transparent;
    width: 8px;
}

QScrollBar:horizontal {
    background: transparent;
    height: 8px;
}

QScrollBar::handle {
    background: palette(mid);
    border-radius: 4px;
}

QScrollBar::handle:pressed {
    background: palette(dark);
}

QScrollBar::add-line,
QScrollBar::sub-line {
    background: none;
}

QScrollBar::add-page,
QScrollBar::sub-page {
    background: none;
}

QSlider::groove:horizontal {
    background-color: palette(mid);
    height: 4px;
	border-radius: 2px;
}

QSlider::handle:horizontal {
    background-color: palette(dark);
    width: 8px;
    margin: -8px 0;
    border-radius: 4px;
}

QSlider::handle:horizontal:pressed {
    background-color: palette(highlight);
}

QSlider::tick:horizontal {
    background-color: palette(dark);
    width: 1px;
}

QHeaderView::section {
    background-color: palette(button);
    color: palette(text);
    padding: 8px;
    border: none;
    border-right: 1px solid rgba(120, 120, 120, 60);
    border-bottom: none;
    height: 30px;
}

QTableView, QTreeView {
    border: none;
    selection-background-color: palette(highlight);
    selection-color: palette(highlighted-text);
}

QTreeView::item {
    min-height: 30px;
    padding: 2px;
}

QPushButton, QLabel[clickable="true"] {
    border-radius: 6px;
    padding: 5px 10px;
}

QLabel[objectName="previewLabel"] {
    font-size: 14px; 
    padding: 5px;
}

QPushButton[objectName="editButton"] {
    border: none; 
    background-color: transparent;
}

QPushButton[objectName="editButton"]:hover {
    background-color: rgba(128, 128, 128, 30);
    border-radius: 20px;
}

QPushButton[objectName="editButton"]:checked {
    background-color: rgba(0, 120, 215, 50);
    border-radius: 20px;
}

QPushButton[objectName="playButton"] {
    border: none; 
    background-color: transparent;
}

QPushButton[objectName="playButton"]:hover {
    background-color: rgba(128, 128, 128, 50);
    border-radius: 20px;
}

QPushButton[objectName="settingsButton"] {
    border: none; 
    background-color: transparent;
}

QPushButton[objectName="settingsButton"]:hover {
    background-color: rgba(128, 128, 128, 30);
    border-radius: 20px;
}

QPushButton[objectName="exportButton"] {
    border: 1px solid gray;
    border-radius: 8px;
    padding: 5px 10px;
}

QPushButton[objectName="exportButton"]:hover {
    background-color: rgba(128, 128, 128, 30);
}

QTableWidget {
    border: none;
    background-color: transparent;
    gridline-color: transparent;
}

QTableWidget::item { 
    padding: 8px;
    min-height: 30px;
}

QTableWidget::item:first-column {
    border-right: 1px solid rgba(120, 120, 120, 60);
}
